    "eslint-plugin-promise": ">=6.0.0",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.10"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --cache",
      "prettier --single-quote --trailing-comma all --write"
    ]
  }
