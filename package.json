    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/sentry": "^5.1.6",
    "@nuxtjs/style-resources": "^1.2.1",
    "@sentry/tracing": "^7.2.0",
    "@vuepic/vue-datepicker": "^3.5.3",
    "@vue-stripe/vue-stripe": "^4.4.4",
